{
  "manifest_version": 2,
  "name": "Manage Bookmarks",
  "description": "Provide actions for managing bookmarks",
  "version": "1.0",
  "author": "gkmobin.net@gmail.com",
  
  "background" : {  "scripts": ["underscore-umd-min.js", "main.js"] },
  
  "__browser_action": {
      "default_title": "Some tooltip",
      "default_popup": "popup.html"
    },
	
  "options_page": "options.html",
  "permissions": [
    "bookmarks",
	"contextMenus"
  ]
}