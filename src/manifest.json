{
  "manifest_version": 3,
  "name": "Manage Bookmarks",
  "description": "Provide actions for managing bookmarks",
  "version": "0.1.0",
  "author": "gkmobin.net@gmail.com",
  
  "background" : {  "serviceworker": ["underscore-umd-min.js", "main.js"] },
	
  "options_page": "options.html",
  "action": {
    "default_popup": "popup/popup.html"
  },

  "permissions": [
    "bookmarks", "tabs"
  ]
}